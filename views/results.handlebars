<div class="container-fluid" id="mainBody">
    <div class="jumbotron text-center bg-dark text-white" id="mainPageHeader">
        <div class="container">
            <h1 class="display-3" id="mainPageHeaderFont">
                <strong>...venUser ..</strong>
            </h1>
        </div>
    </div>
</div>
<table class="table table-light">
    <thead>
        <tr>
            
            <th scope="col">Venue:</th>
            <th scope="col">Address:</th>
        </tr>
    </thead>
    <tbody>
        {{#each venue}}
        <tr data-id={{this.id}}>
            <td><form method="post" action="/venues/{{this.id}}">
                <input type="hidden" name="venueid" value="{{this.id}}">
                <button type="submit" name="submit_param" value="submit_value">
                    {{this.name}}
                </button>
            </form>
            {{!-- <a href="/venues/{{this.id}}">{{this.name}}</a> --}}
            </td>
            <td>{{this.address}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>