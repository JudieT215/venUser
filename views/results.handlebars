<div class="container-fluid" id="mainBody">
    <div class="jumbotron text-center bg-dark text-white" id="mainPageHeader">
        <div class="container">
            <h1 class="display-3" id="mainPageHeaderFont">
                <strong>...venUser ..</strong>
            </h1>
        </div>
    </div>
</div>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col"></th>
            <th scope="col">#</th>
            <th scope="col">Venue</th>
        </tr>
    </thead>
    <tbody>
        {{#each venue}}
        <tr data-id={{this.id}}>
            <td><a href="/venues/{{this.id}}">{{this.name}}</a></td>
            <td>{{this.address}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>