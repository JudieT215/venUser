        <h2>Add a Review:</h2>
    </div>
</div>
<br>
<div class="row" id="reviewArea"></div>
<div class="col-sm-3"></div>
<div class="col-sm-3">
    <a href="{{venueObj.review.name_author}}"></a>
</div>
<div class="col-sm-3">{{venueObj.review.content}} </div>
<div class="col-sm-3"></div>
{{/each}}
</div>